  "type": "module",
  "scripts": {
    "dev": "vite dev --port 5001",
    "dev:server": "NODE_ENV=development tsx server/index.ts",
    "build": "vite build",
    "build:static": "node script/fetch-questions-for-build.js && node script/generate-rss.js && node script/generate-sitemap.js && vite build && node script/generate-pagefind-index.js && node script/build-pagefind.js",
    "build:pagefind": "node script/generate-pagefind-index.js && node script/build-pagefind.js",
    "fetch:data": "node script/fetch-questions-for-build.js",
    "build:server": "tsx script/build.ts",
    "start": "NODE_ENV=production node dist/index.cjs",
    "check": "tsc",
    "db:migrate": "node script/migrate-questions-to-turso.js",
    "db:push": "drizzle-kit push",
    "deploy:pages": "tsx script/deploy-pages.ts",
    "preview": "rm -f dist/public/404.html && vite preview --port 3333",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "validate:questions": "node script/validate-questions.js",
    "fix:duplicates": "node script/fix-duplicate-questions.js"
  },
